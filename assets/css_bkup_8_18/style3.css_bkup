.right {float: right !important;}
/* Image zoom on hover + Overlay colour */
.parent {
    width: 27%;
    margin: 27px;
    height: 227px;
    overflow: hidden;
    position: relative;
    float: left;
    display: inline-block;
    cursor: pointer;
    border-radius: 45px;
    margin-top: 4%;
}

.child {
    height: 100%;
    width: 100%;
    background-size: cover;
    background-repeat: no-repeat;
    -webkit-transition: all .5s;
    -moz-transition: all .5s;
    -o-transition: all .5s;
    transition: all .5s;
}
                                                                                            
