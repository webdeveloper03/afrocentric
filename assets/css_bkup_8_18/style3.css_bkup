/*switch on-off */
.toggle-label {
  position: relative;
    width: 200px;
    height: 30px;
    margin-top: 10px;
}
.toggle-label input[type=checkbox] { 
    opacity: 0;
    position: absolute;
    width: 100%;
    height: 100%;
    left: 0;
    top: 0;
}
.toggle-label input[type=checkbox]+.back {
    position: absolute;
    width: 75%;
    height: 30px;
    left: 0;
    top: 0;
    border-radius: 50px;
    border: 0px solid #000;
    transition: background 150ms linear;
    background: #D4D4D4;
}
.toggle-label input[type=checkbox]:checked+.back {
    border: 0px solid #000;
    cursor: pointer;
}

.toggle-label input[type=checkbox]+.back .toggle {
  display: block;
    position: absolute;
    content: ' ';
    background: #FFAF2E;
    width: 50%;
    height: 100%;
    transition: margin 150ms linear;
    border: 0px solid #808080;
    border-radius: 50px;
    color: #000 !important;
    text-align: center;
}
.toggle-label input[type=checkbox]:checked+.back .toggle {
  margin-left: 74px;
}
.toggle-label .label {
  display: block;
    position: absolute;
    width: 40%;
    color: black;
    line-height: 30px;
    text-align: center;
    font-size: 12px;
    font-family: 'Proxima-Nova-Bold' !important;
}
.toggle-label .label.on { left: 8px; }
.toggle-label .label.off { 
    right: 8px;
    color: #000 !important;
    font-family: "ProximaNova-Regular" !important;
}

.toggle-label input[type=checkbox]:checked+.back .label.on {
